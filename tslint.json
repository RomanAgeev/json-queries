{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "max-line-length": false,
        "max-classes-per-file": false,
        "member-ordering": false,
        "variable-name": false,
        "member-access": false,
        "array-type": false,
        "align": false,
        "interface-over-type-literal": false
    },
    "rulesDirectory": []
}